/dts-v1/;

#include "imx6dl.dtsi"

/ {
	model = "AQS Novena Solo/DualLite";
	compatible = "aqs,novena-dl", "kosagi,novena-dl", "fsl,imx6dl";

	memory {
		reg = <0x10000000 0xefffffff>;
	};

	soc {
		aips-bus@0200000 {
			snvs@020cc000 {
				snvs-rtc-lp@34 {
					status = "disabled";
				};
			};
		};
	};

	regulators {
		compatible = "simple-bus";

		reg_2p5v: 2p5v {
			compatible = "regulator-fixed";
			regulator-name = "2P5V";
			regulator-min-microvolt = <2500000>;
			regulator-max-microvolt = <2500000>;
			regulator-always-on;
		};

		reg_3p3v: 3p3v {
			compatible = "regulator-fixed";
			regulator-name = "3P3V";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};

		reg_usb_otg_vbus: usb_otg_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb_otg_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			enable-active-high;
		};
		reg_lvds_backlignt: lcd_backlight_regulator {
			compatible = "regulator-fixed";
			regulator-name = "lcd-backlight-power";
			regulator-min-microvolt = <12000000>;
			regulator-max-microvolt = <12000000>;
			gpio = <&gpio4 15 0>;
			enable-active-high;
			regulator-boot-on;
		};
		reg_lvds_lcd: lcd_pwr_ctrl {
			compatible = "regulator-fixed";
			regulator-name = "lcd-lvds-power";
			regulator-min-microvolt = <12000000>;
			regulator-max-microvolt = <12000000>;
			startup-delay-us = <50000>;
			gpio = <&gpio5 28 0>;
			enable-active-high;
			regulator-boot-on;
		};
	};

	imx-drm {
		compatible = "fsl,imx-drm";
		crtcs = <&ipu1 0>, <&ipu1 1>;
		connectors = <&ldb>;
	};

	leds {
		compatible = "gpio-leds";
		heartbeat {
			label = "novena::usr0";
			gpios = <&gpio3 19 0>;
			linux,default-trigger = "heartbeat";
		};
	};
	leds-novds {
		compatible = "leds-novds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_leds_novds>;
	};


	backlight {
		compatible = "pwm-backlight";
		pwms = <&pwm1 0 10000000>;

		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_backlight_novena>;
		power-supply = <&reg_lvds_backlignt>;

		brightness-levels = <0 40 50 60 68 76 84 92 100 110 120>;
		default-brightness-level = <10>;
	};


	rotary@0 {
		compatible = "rotary-encoder";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_novds_wheel>;
		gpios = <&gpio2 21 0>, <&gpio1 02 0>, <&gpio2 19 0>;
		linux,axis = <8>; /* REL_WHEEL */
		rotary-encoder,relative-axis;
		rotary-encoder,half-period;
	};

	sound {
		compatible = "fsl,imx-audio-sgtl5000",
			     "fsl,imx-noventds-sgtl5000";
		model = "imx6dl-noventds-sgtl5000";
		ssi-controller = <&ssi1>;
		audio-codec = <&codec>;
		audio-routing =
			"MIC_IN", "Mic Jack",
			"Mic Jack", "Mic Bias",
			"Line out", "LINE_OUT";
		mux-int-port = <1>;
		mux-ext-port = <3>;
	};
};

&ssi1 {
	fsl,mode = "i2s-slave";
	status = "okay";
};

&uart2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2_novena>;
};

&uart3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart3_novena>;
};

&uart4 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart4_novena>;
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog>;

	backlight {
		pinctrl_backlight_novena: backlight-pwm {
			fsl,pins = <
				MX6QDL_PAD_DISP0_DAT8__PWM1_OUT   0x1b0b0
				MX6QDL_PAD_CSI0_DAT10__GPIO5_IO28 0x80000000
				MX6QDL_PAD_KEY_ROW4__GPIO4_IO15   0x80000000
			>;
		};
	};

	enet {
		pinctrl_enet_novena: enetgrp-novena {
			fsl,pins = <
				MX6QDL_PAD_ENET_MDIO__ENET_MDIO       0x1b0b0
				MX6QDL_PAD_ENET_MDC__ENET_MDC         0x1b0b0
				MX6QDL_PAD_RGMII_TXC__RGMII_TXC       0x1b020
				MX6QDL_PAD_RGMII_TD0__RGMII_TD0       0x1b028
				MX6QDL_PAD_RGMII_TD1__RGMII_TD1       0x1b028
				MX6QDL_PAD_RGMII_TD2__RGMII_TD2       0x1b028
				MX6QDL_PAD_RGMII_TD3__RGMII_TD3       0x1b028
				MX6QDL_PAD_RGMII_TX_CTL__RGMII_TX_CTL 0x1b028
				MX6QDL_PAD_ENET_REF_CLK__ENET_TX_CLK  0x1b0b0
				MX6QDL_PAD_RGMII_RXC__RGMII_RXC       0x1b0b0
				MX6QDL_PAD_RGMII_RD0__RGMII_RD0       0x1b0b0
				MX6QDL_PAD_RGMII_RD1__RGMII_RD1       0x1b0b0
				MX6QDL_PAD_RGMII_RD2__RGMII_RD2       0x1b0b0
				MX6QDL_PAD_RGMII_RD3__RGMII_RD3       0x1b0b0
				MX6QDL_PAD_RGMII_RX_CTL__RGMII_RX_CTL 0x1b0b0
				MX6QDL_PAD_GPIO_16__ENET_REF_CLK      0x4001b0a8

				/* Ethernet reset */
				MX6QDL_PAD_EIM_D23__GPIO3_IO23  0x80000000
			>;
		};
	};

	i2c1 {
		pinctrl_i2c1_novena: i2c1grp-novena {
			fsl,pins = <
				MX6QDL_PAD_EIM_D21__I2C1_SCL 0x4001b8b1
				MX6QDL_PAD_EIM_D28__I2C1_SDA 0x4001b8b1
			>;
		};
	};
	i2c2 {
		pinctrl_i2c2_novena: i2c2grp-novena {
			fsl,pins = <
				MX6QDL_PAD_EIM_EB2__I2C2_SCL 0x4001b8b1
				MX6QDL_PAD_EIM_D16__I2C2_SDA 0x4001b8b1
			>;
		};
	};

	i2c3 {
		pinctrl_i2c3_novena: i2c3grp-novena {
			fsl,pins = <
				MX6QDL_PAD_EIM_D17__I2C3_SCL 0x4001b8b1
				MX6QDL_PAD_EIM_D18__I2C3_SDA 0x4001b8b1
			>;
		};
	};

	hog {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				/* Touchscreen interrupt */
				MX6QDL_PAD_DISP0_DAT19__GPIO5_IO13 0x80000000

				/* zero valve */
				MX6QDL_PAD_DISP0_DAT5__GPIO4_IO26 0X80000000

				/* water full detector */
				MX6QDL_PAD_EIM_A16__GPIO2_IO22  0X80000000

				/* backup switch detector */
				MX6QDL_PAD_CSI0_DAT16__GPIO6_IO02 0X80000000

				/* power enable for delivery board */
				MX6QDL_PAD_DI0_PIN3__GPIO4_IO19 0X80000000
			>;
		};
	};

	pcie {
		pinctrl_pcie_novena: pciegrp-novena {
			fsl,pins = <
				/* Wakeup */
				MX6QDL_PAD_EIM_D22__GPIO3_IO22  0x80000000

				/* Reset */
				MX6QDL_PAD_EIM_D29__GPIO3_IO29  0x80000000

				/* Power On */
				MX6QDL_PAD_GPIO_17__GPIO7_IO12  0x80000000

				/* Wifi kill */
				MX6QDL_PAD_EIM_A22__GPIO2_IO16  0x80000000
			>;
		};
	};

	sound {
		pinctrl_sound_novena: sound-novena {
			fsl,pins = <
				/* Audio power regulator */
				MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17 0x80000000

				/* Headphone plug */
				MX6QDL_PAD_DISP0_DAT21__GPIO5_IO15 0x80000000

				/* Speaker amplifier mute */
				MX6QDL_PAD_KEY_ROW1__GPIO4_IO09    0x80000000

				/* Audio mclk */
				MX6QDL_PAD_GPIO_0__CCM_CLKO1       0x80000000
			>;
		};
	};

	uart2 {
		pinctrl_uart2_novena: uart2grp-novena {
			fsl,pins = <
				MX6QDL_PAD_EIM_D26__UART2_TX_DATA   0x1b0b1
				MX6QDL_PAD_EIM_D27__UART2_RX_DATA   0x1b0b1
			>;
		};
	};

	uart3 {
		pinctrl_uart3_novena: uart3grp-novena {
			fsl,pins = <
				MX6QDL_PAD_EIM_D24__UART3_TX_DATA 0x1b0b1
				MX6QDL_PAD_EIM_D25__UART3_RX_DATA 0x1b0b1
			>;
		};
	};
	uart4 {
		pinctrl_uart4_novena: uart4-novena {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT12__UART4_TX_DATA 0x1b0b1
				MX6QDL_PAD_CSI0_DAT13__UART4_RX_DATA 0x1b0b1
			>;
		};
	};

	usdhc3 {
		pinctrl_usdhc3_novena: usdhc3-novena {
			fsl,pins = <
				MX6QDL_PAD_SD2_CMD__SD2_CMD    0x17059
				MX6QDL_PAD_SD2_CLK__SD2_CLK    0x10059
				MX6QDL_PAD_SD2_DAT0__SD2_DATA0 0x17059
				MX6QDL_PAD_SD2_DAT1__SD2_DATA1 0x17059
				MX6QDL_PAD_SD2_DAT2__SD2_DATA2 0x17059
				MX6QDL_PAD_SD2_DAT3__SD2_DATA3 0x17059
			>;
		};
	};
	usdhc2 {
		pinctrl_novena_usdhc2: novena-usdhc2 {
			fsl,pins = <
				MX6QDL_PAD_SD3_CMD__SD3_CMD	0x17059
				MX6QDL_PAD_SD3_CLK__SD3_CLK	0x10059
				MX6QDL_PAD_SD3_DAT0__SD3_DATA0	0x17059
				MX6QDL_PAD_SD3_DAT1__SD3_DATA1	0x17059
				MX6QDL_PAD_SD3_DAT2__SD3_DATA2	0x17059
				MX6QDL_PAD_SD3_DAT3__SD3_DATA3	0x17059
				MX6QDL_PAD_SD3_DAT4__SD3_DATA4	0x17059
				MX6QDL_PAD_SD3_DAT5__SD3_DATA5	0x17059
				MX6QDL_PAD_SD3_DAT6__SD3_DATA6	0x17059
				MX6QDL_PAD_SD3_DAT7__SD3_DATA7	0x17059
				MX6QDL_PAD_SD3_RST__GPIO7_IO08	0x80000000
			>;
		};

		pinctrl_novena_usdhc2_100mhz: novena-usdhc3-100mhz {
			fsl,pins = <
				MX6QDL_PAD_SD3_CMD__SD3_CMD	0x170b9
				MX6QDL_PAD_SD3_CLK__SD3_CLK	0x100b9
				MX6QDL_PAD_SD3_DAT0__SD3_DATA0	0x170b9
				MX6QDL_PAD_SD3_DAT1__SD3_DATA1	0x170b9
				MX6QDL_PAD_SD3_DAT2__SD3_DATA2	0x170b9
				MX6QDL_PAD_SD3_DAT3__SD3_DATA3	0x130b9
				MX6QDL_PAD_SD3_DAT4__SD3_DATA4	0x170b9
				MX6QDL_PAD_SD3_DAT5__SD3_DATA5	0x170b9
				MX6QDL_PAD_SD3_DAT6__SD3_DATA6	0x170b9
				MX6QDL_PAD_SD3_DAT7__SD3_DATA7	0x130b9
				MX6QDL_PAD_SD3_RST__GPIO7_IO08	0x80000000
			>;
		};

		pinctrl_novena_usdhc2_200mhz: novena-usdhc3-200mhz {
			fsl,pins = <
				MX6QDL_PAD_SD3_CMD__SD3_CMD	0x170f9
				MX6QDL_PAD_SD3_CLK__SD3_CLK	0x100f9
				MX6QDL_PAD_SD3_DAT0__SD3_DATA0	0x170f9
				MX6QDL_PAD_SD3_DAT1__SD3_DATA1	0x170f9
				MX6QDL_PAD_SD3_DAT2__SD3_DATA2	0x170f9
				MX6QDL_PAD_SD3_DAT3__SD3_DATA3	0x130f9
				MX6QDL_PAD_SD3_DAT4__SD3_DATA4	0x170f9
				MX6QDL_PAD_SD3_DAT5__SD3_DATA5	0x170f9
				MX6QDL_PAD_SD3_DAT6__SD3_DATA6	0x170f9
				MX6QDL_PAD_SD3_DAT7__SD3_DATA7	0x130f9
				MX6QDL_PAD_SD3_RST__GPIO7_IO08	0x80000000
			>;
		};
	};
	novds_wheel {
		pinctrl_novds_wheel: novds-wheel {
			fsl,pins = <
				MX6QDL_PAD_EIM_A17__GPIO2_IO21  0x80000000
				MX6QDL_PAD_EIM_A19__GPIO2_IO19  0x80000000
				MX6QDL_PAD_GPIO_2__GPIO1_IO02   0x80000000
			>;
		};
	};
	leds_novds {
		pinctrl_leds_novds: leds-novds {
			fsl,pins = <
				MX6QDL_PAD_EIM_LBA__GPIO2_IO27 0x80000000
				MX6QDL_PAD_DISP0_DAT11__GPIO5_IO05 0x80000000
				MX6QDL_PAD_EIM_RW__GPIO2_IO26 0x80000000
			>;
		};
	};

};

&fec {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet_novena>;
	phy-mode = "rgmii";
	phy-reset-gpios = <&gpio3 23 0>;
	mac-address = [00 00 00 00 00 00];
	rxc-skew-ps = <3000>;
	rxdv-skew-ps = <0>;
	txc-skew-ps = <3000>;
	txen-skew-ps = <0>;
	rxd0-skew-ps = <0>;
	rxd1-skew-ps = <0>;
	rxd2-skew-ps = <0>;
	rxd3-skew-ps = <0>;
	txd0-skew-ps = <3000>;
	txd1-skew-ps = <3000>;
	txd2-skew-ps = <3000>;
	txd3-skew-ps = <3000>;
};

/* iNAND */
&usdhc2 {
    pinctrl-names = "default", "state_100mhz", "state_200mhz";
    pinctrl-0 = <&pinctrl_novena_usdhc2>;
    pinctrl-1 = <&pinctrl_novena_usdhc2_100mhz>;
    pinctrl-2 = <&pinctrl_novena_usdhc2_200mhz>;
	non-removable;
	bus-width = <1>;
	card-reset-gpios = <&gpio7 8 1>;
	vmmc-supply = <&reg_3p3v>;
	status = "okay";
};


/* Removable MicroSD */
&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3_novena>;
	non-removable;
	vmmc-supply = <&reg_3p3v>;
	status = "okay";
};

&usbotg {
	vbus-supply = <&reg_usb_otg_vbus>;
	dr_mode = "otg";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg_2>;
	disable-over-current;
	status = "okay";
};

&usbh1 {
	status = "okay";
	vbus-supply = <&swbst_reg>;
};

&audmux {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_audmux_2>;
};

&i2c1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1_novena>;
	status = "okay";

	stmpe811@44 {
		status = "okay";
		compatible = "st,stmpe811";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x44>;
		irq-over-gpio;
		irq-gpios = <&gpio5 13 0>;
		irq-trigger = <0x1>;
		id = <0>;
		blocks = <0x5>;

		stmpe_touchscreen {
			compatible = "st,stmpe-ts";
			st,sample-time = <4>;
			st,mod-12b = <1>;
			st,ref-sel = <0>;
			st,adc-freq = <1>;
			st,ave-ctrl = <1>;
			st,touch-det-delay = <2>;
			st,settling = <2>;
			st,fraction-z = <7>;
			st,i-drive = <1>;
		};
	};

	ds1340@68 {
		compatible = "dallas,ds1340";
		status = "okay";
		reg = <0x68>;
	};
	ads7828@48 {
		compatible = "ads7828";
		status = "okay";
		reg = <0x48>;
		vref_mv = "2500";
	};
};

&i2c2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2_novena>;
	status = "okay";

	pmic: pfuze100@08 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;
		status = "okay";

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3a_reg: sw3a {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3b_reg: sw3b {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5000000>;
				regulator-max-microvolt = <5150000>;
				regulator-boot-on;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3000000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-boot-on;
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			coin_reg: coin {
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
				regulator-boot-on;
			};
		};
	};
};

&i2c3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3_novena>;
	status = "okay";

	codec: sgtl5000@0a {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_sound_novena>;
		compatible = "fsl,sgtl5000";
		reg = <0x0a>;
		clocks = <&clks 201>;
		VDDD-supply = <&vgen3_reg>;
		VDDA-supply = <&reg_2p5v>;
		VDDIO-supply = <&reg_3p3v>;
	};

	msp430@68 {
		compatible = "novds,msp430";
		status = "okay";
		reg = <0x68>;
		irq-over-gpio;
		irq-gpios = <&gpio4 18 0>;
		irq-trigger = <0x1>;
	};
};

&ecspi3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi3_1>;
	fsl,spi-num-chipselects = <3>;
	status = "okay";

	spidev@0 {
		compatible = "spidev";
		spi-max-frequency = <30000000>;
		reg = <0>;
	};
};

&ldb {
	status = "okay";

	lvds-channel@0 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <24>;
		status = "okay";

		display-timings {
			native-mode = <&timing0>;
			timing0: 800x480p60 {
				clock-frequency = <29500000>;
				hactive = <800>;
				vactive = <480>;
				hback-porch = <100>;
				hfront-porch = <50>;
				hsync-len = <42>;
				vback-porch = <10>;
